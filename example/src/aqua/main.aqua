module Main

export and


func and(l: bool, r: bool) -> bool:
    res: ?bool
    if l:
        res <<- r
    else:
        res <<- false
    <- res!

func or(l: bool, r: bool) -> bool:
    res: ?bool
    if l:
        res <<- true
    else:
        res <<- r
    <- res!

func not(u: bool) -> bool:
    res: ?bool
    if u:
        res <<- false
    else:
        res <<- true
    <- res!
